@Article{Bra98,
  Title                    = {{The equations $A^{T}X\pm X^{T}A=B$}},
  Author                   = {Braden, H.W.},
  Journal                  = SIAMMatrix,
  Year                     = {1998},
  Number                   = {2},
  Pages                    = {295-302},
  Volume                   = {20},

