@Book{Lay08,
  Title                    = {{Introduction to the Numerical Analysis of Incompressible Viscous Flows}},
  Author                   = {Layton, William},
  Publisher                = {SIAM},
  Year                     = {2008},

  Address                  = {Philadelphia, PA},

  Abstract                 = {{According to the author, this book was written ``to help graduate students who feel they are up to the chalenge of the beautiful and complex world of CFD". Thus, the purpose of the book is to allow graduate students to progress from essentially zero to finite element CFD, and even to include one advanced topic in the field (such as turbulence) in one academic term. The book goal is to connect ideas from the numerical analysis of Navier--Stokes equations (NSE) without losing understanding what fluid flow simulations really mean. Each chapter is self-contained as possible. Part I begins with a chapter devoted to mathematical preliminaries. Chapter 2 introduces FEM in detail to begin the treatment of NSE. Chapter 3 covers and reviews vectors, tensors, and conservation laws, which is necessary to go farther in CFD. Part II opens with Chapter 4, which presents mixed methods for Stokes problem. A derivation of NSE and the properties of solutions that are essential to understand their numerical features are the topics of Chapter 5. The theory of steady NSE and their numerical analysis are the aim of Chapter 6 and 7, respectively. Part III considers time-dependent fluid flows beginning with a summary of the Leray theory for NSE in Chapter 8. Chapter 8 proves also stability of finite element methods for time-dependent NSE, backing upon the energy equality. The final topic mentions the enormous variety of fluid flow phenomena from the turbulence. But the author restricts himself to the presentation of homogeneous isotropic turbulence (Chapter 10). Each chapter on the theory is followed by a chapter on numerical analysis that expands the theory. The chapters contain numerous exercises. With mathematical rigor and physical clarity, this book provides a useful tool for understanding the foundations of methods commonly used for flow simulations.}},
  Classmath                = {{*76-01 (Textbooks (fluid mechanics)) 76M10 (Finite element methods) 76D05 (Navier--Stokes equations (fluid dynamics)) 76F05 (Homogeneous isotropic turbulence) }},
  Keywords                 = {{Navier--Stokes equations; finite element methods; homogeneous isotropic turbulence}},
  Language                 = {English},
  Reviewer                 = {{Titus Petrila (Cluj-Napoca)}}
}

