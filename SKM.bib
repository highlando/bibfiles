  keywords = {{nonlinear systems; existence and uniquenes of solutions; iterative
	mathods; convergence; minimization problems; gradient methods; contraction
	mappings theorems; textbook; reprint of classics; bibliography; exercises;
	continuation methods; partially ordered linear space}},
  language = {English},
  reviewer = {{Nelli Dimitrova (Sofia)}}
}

